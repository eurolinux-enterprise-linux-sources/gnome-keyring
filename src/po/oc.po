#, c-format
msgid "waiting for lock (held by %d%s) %s...\n"
msgstr "en espèra del verrolhatge (tengut per %d%s) %s...\n"

#: ../egg/dotlock.c:1085
msgid "(deadlock?) "
msgstr "(blocatge ?) "

#: ../egg/dotlock.c:1124
#, c-format
